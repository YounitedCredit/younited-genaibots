GET_ALL_THREAD_FROM_MESSAGE_LINKS=True  # Fetch all thread messages from message links
ACTIVATE_MESSAGE_QUEUING = True # Activate message queuing, if False, the bot won't treat message until previous message is processed or TTL time is reached

